 ALGORITHM Sort
 VAR
  i,j:INTEGER;
  valueToInsert :INTEGER :=0;

  BEGIN
  
  FOR i FROM 1 TO N-1 DO STEP 1
  valueToInsert = arr[i]
   j = i - 1
   WHILE (j >= 0 and arr[j] > valueToInsert) DO
       arr[j+1] = arr[j]
       j = j - 1
   END_WHILE
   arr[j+1] = valueToInsert
   END_FOR
END

 
 
 